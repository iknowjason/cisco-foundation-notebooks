  {
   "cell_type": "code",
   "execution_count": null,
   "id": "batch_analysis_cell",
   "metadata": {},
   "outputs": [],
   "source": [
    "def analyze_multiple_jobs(job_descriptions, analysis_type=\"comprehensive\"):\n",
    "    \"\"\"Analyze multiple job descriptions for OSINT intelligence\"\"\"\n",
    "    \n",
    "    results = []\n",
    "    \n",
    "    for i, job_desc in enumerate(job_descriptions, 1):\n",
    "        print(f\"\\n=== ANALYZING JOB DESCRIPTION {i}/{len(job_descriptions)} ===\")\n",
    "        \n",
    "        if analysis_type == \"comprehensive\":\n",
    "            prompt = f'''Provide a comprehensive OSINT analysis of this job description focusing on:\n",
    "            1. Technology stack identification\n",
    "            2. Security implications\n",
    "            3. Attack vector assessment\n",
    "            4. Intelligence value rating (1-10)\n",
    "            \n",
    "            <Job Description>\n",
    "            {job_desc}\n",
    "            '''\n",
    "        elif analysis_type == \"quick\":\n",
    "            prompt = f'''Provide a quick OSINT assessment of this job description:\n",
    "            - Key technologies mentioned\n",
    "            - Primary security concerns\n",
    "            - Intelligence value (High/Medium/Low)\n",
    "            \n",
    "            <Job Description>\n",
    "            {job_desc}\n",
    "            '''\n",
    "        \n",
    "        analysis = inference(prompt)\n",
    "        results.append({\n",
    "            'job_number': i,\n",
    "            'description': job_desc[:200] + \"...\" if len(job_desc) > 200 else job_desc,\n",
    "            'analysis': analysis\n",
    "        })\n",
    "        \n",
    "        print(f\"Job {i} analysis complete.\")\n",
    "    \n",
    "    return results\n",
    "\n",
    "# Example job descriptions for batch analysis\n",
    "sample_job_descriptions = [\n",
    "    \"\"\"Senior Software Engineer - We are looking for a Senior Software Engineer to work with our microservices architecture using Docker, Kubernetes, and AWS. Experience with Python, Go, and PostgreSQL required.\"\"\",\n",
    "    \n",
    "    \"\"\"Cloud Security Architect - Join our team to design secure cloud infrastructures using Terraform, implement Zero Trust architectures, and manage our SIEM deployment with Splunk.\"\"\",\n",
    "    \n",
    "    \"\"\"DevSecOps Engineer - Looking for someone to integrate security into our CI/CD pipelines using Jenkins, implement infrastructure as code, and manage our vulnerability scanning tools.\"\"\"\n",
    "]\n",
    "\n",
    "if sample_job_descriptions:\n",
    "    print(\"Starting batch analysis of sample job descriptions...\")\n",
    "    batch_results = analyze_multiple_jobs(sample_job_descriptions, analysis_type=\"quick\")\n",
    "    \n",
    "    print(\"\\n\" + \"=\"*60)\n",
    "    print(\"BATCH ANALYSIS SUMMARY\")\n",
    "    print(\"=\"*60)\n",
    "    \n",
    "    for result in batch_results:\n",
    "        print(f\"\\n### Job {result['job_number']}\")\n",
    "        print(f\"**Description:** {result['description']}\")\n",
    "        print(f\"**Analysis:**\")\n",
    "        display(Markdown(result['analysis']))\n",
    "        print(\"-\" * 40)"
